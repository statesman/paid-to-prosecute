                <!-- begin member_center_override block --> 
                <script>
    
    window.preMemberCenter = (function () {
        var s = 'script', callbacks = [], sibling, subSub, mainUrl = '',
            mcScript = document.createElement(s),
            envPropertyNiche = false,
            currentSiteOverride = '',
            currentSite = currentSiteOverride || 'http://www.mystatesman.com',
            mc_prod = true,
            urlParts = currentSite.split('.');

        if (true) {
            if (envPropertyNiche) {
                subSub = '-' + urlParts[0].split(/-|\/\//).pop();
            }
            urlParts[0] = 'https://membercenter' + (mc_prod ? '' : '-staging') + (subSub ? subSub : '');
            mainUrl = urlParts.join('.');
            mcScript.src = mainUrl + '/sdk.js';
        } else {
            var site = 'mystatesman';
            mcScript.src = 'http://connext-promises.aws.cmgdigital.com/sdk.js?_tenant=' + site;
        }
        
        mcScript.onload = function () {
            var a = callbacks; 
            callbacks = null;
            a.forEach(function (fn) { fn(); });
        };
        mcScript.onerror = function () {
            throw new Error('Failed to fetch Member Center SDK from ' + mainUrl);
        };
        
        sibling = document.getElementsByTagName(s)[0];
        sibling.parentNode.insertBefore(mcScript, sibling);

        return {
            onReady: function (fn) { callbacks ? callbacks.push(fn) : fn() },
            mainUrl: mainUrl
        }   
    })();
</script>

